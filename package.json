{
  "name": "express-starter",
  "version": "0.0.1",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "eslint src"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "no-debugger": 1,
      "no-duplicate-case": 1,
      "no-empty": [1, {
        "allowEmptyCatch": true
      }],
      "no-ex-assign": 1,
      "no-extra-boolean-cast": 1,
      "no-extra-parens": 1,
      "no-extra-semi": 1,
      "no-inner-declarations": 1,
      "no-irregular-whitespace": 1,
      "no-prototype-builtins": 1,
      "no-unsafe-negation": 1,
      "accessor-pairs": 1,
      "block-scoped-var": 1,
      "consistent-return": 1,
      "dot-notation": 1,
      "no-alert": 2,
      "no-else-return": 1,
      "no-empty-function": 1,
      "no-global-assign": 2,
      "no-magic-numbers": [1, { "ignore": [1] }],
      "no-multi-spaces": [1, {
        "exceptions": {
          "Property": false
        }
      }],
      "no-proto": 1,
      "no-param-reassign": 1,
      "no-return-assign": 1,
      "no-void": 1,
      "wrap-iife": 1,
      "yoda": 1,
      "block-spacing": 1,
      "array-bracket-spacing": 1,
      "brace-style": [1, "stroustrup"],
      "camelcase": 1,
      "comma-dangle": [1, "always-multiline"],
      "comma-spacing": [1, {
        "before": false, "after": true
      }],
      "comma-style": [1, "last"],
      "eol-last": [1, "always"],
      "func-call-spacing": 1,
      "func-style": [1, "expression", {
        "allowArrowFunctions": true
      }],
      "jsx-quotes": 1,
      "key-spacing": 1,
      "indent": [1, 2],
      "max-depth": [1, 3],
      "max-statements-per-line": [1, {
        "max": 1
      }],
      "new-cap": 1,
      "newline-after-var": 1,
      "newline-before-return": 1,
      "no-negated-condition": 1,
      "no-multiple-empty-lines": [1, {
        "max": 1
      }],
      "no-mixed-spaces-and-tabs": 1,
      "no-nested-ternary": 1,
      "no-whitespace-before-property": 1,
      "object-property-newline": 1,
      "one-var-declaration-per-line": 1,
      "padded-blocks": [1, "never"],
      "quotes": 1,
      "semi-spacing": 1,
      "semi": 1,
      "sort-keys": [1, "asc", {
        "natural": true
      }],
      "sort-vars": 1,
      "space-before-blocks": 1,
      "space-before-function-paren": [1, {
        "named": "ignore"
      }],
      "space-infix-ops": 1,
      "space-unary-ops": 1,
      "wrap-regex": 1,
      "arrow-parens": 1,
      "arrow-spacing": 1,
      "generator-star-spacing": 1,
      "no-duplicate-imports": 1,
      "no-useless-computed-key": 1,
      "object-shorthand": 1,
      "prefer-const": 1,
      "prefer-rest-params": 1,
      "prefer-spread": 1,
      "prefer-template": 1,
      "require-yield": 1,
      "rest-spread-spacing": 1,
      "sort-imports": [1, {
        "memberSyntaxSortOrder": ["all", "multiple", "single", "none"]
      }]
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ugogo/express-starter"
  },
  "author": "Ugo Onali",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "dotenv": "^2.0.0",
    "express": "^4.13.3",
    "mongoose": "^4.7.2",
    "shortid": "^2.2.6"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.1.1",
    "nodemon": "^1.9.2"
  },
  "bugs": {
    "url": "https://github.com/ugogo/express-starter/issues"
  },
  "homepage": "https://github.com/ugogo/express-starter#readme",
  "keywords": []
}
